          <stringProp name="script">import org.apache.jmeter.samplers.SampleResult;

int totalRequests = ctx.getPreviousResult().getTotal();
int failedRequests = ctx.getPreviousResult().getFailureCount();
double errorRate = (failedRequests * 100.0) / totalRequests;

if (errorRate &gt; 1) {
    Failure = true;
    FailureMessage = &quot;Taxa de erro excedeu 1%: &quot; + errorRate + &quot;%&quot;;
}
