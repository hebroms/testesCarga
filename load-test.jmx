          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.samplers.SampleResult

// Função para calcular percentil 95 manualmente
def calculatePercentile(samples, percentile) {
    if (samples.isEmpty()) return 0
    def sortedSamples = samples.sort() // Ordena os tempos de resposta
    def index = (percentile / 100.0 * (sortedSamples.size() - 1)).round()
    return sortedSamples[index]
}

// Obtém os resultados das amostras
def sampleResults = ctx.getPreviousResult().getSubResults()
def responseTimes = sampleResults.collect { it.getTime() } // Coleta os tempos de resposta

// Cálculo do percentil 95
def responseTime95 = calculatePercentile(responseTimes, 95)

// Obtém contagens globais
def totalRequests = sampleResults.size() // Total de requisições realizadas
def failedRequests = sampleResults.count { it.isSuccessful() == false } // Requisições com erro
def errorRate = (failedRequests / totalRequests) * 100 // Taxa de erro

// Critérios de aceitação
def maxResponseTime = 2000 // Tempo máximo para 95% das requisições (em milissegundos)
def maxErrorRate = 1.0 // Taxa de erro máxima permitida (em %)

// Valida os critérios
if (responseTime95 &gt; maxResponseTime || errorRate &gt; maxErrorRate) {
    AssertionResult.setFailure(true)
    AssertionResult.setFailureMessage(
        &quot;Critérios de sucesso não atendidos: &quot; +
        &quot;95% do tempo de resposta = ${responseTime95}ms (máximo permitido: ${maxResponseTime}ms), &quot; +
        &quot;ou taxa de erro = ${errorRate}% (máximo permitido: ${maxErrorRate}%)&quot;
    )
} else {
    log.info(&quot;Critérios atendidos: Percentil 95 = ${responseTime95}ms, Taxa de Erro = ${errorRate}%&quot;)
}
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Assertion>
